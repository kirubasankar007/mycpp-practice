#include<iostream>
#include<iomanip>
using namespace std;
int main()
{
    int n=3;
    int arr[n][n];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
           cin>>arr[i][j];
        }
    }
    if(arr[1][0]==0&&arr[2][0]==0&&arr[2][1]==0)
    {
        cout<<"yes";
        
    }
    else
    {
        cout<<"no";
    }
}

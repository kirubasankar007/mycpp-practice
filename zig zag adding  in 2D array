#include<iostream>
#include<iomanip>
#include<cmath>
using namespace std;
int main()
{
    int n=3,sum=0;
    int arr[n][n];
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<n;j++)
        {
           cin>>arr[i][j];
        }
    }
    sum=arr[0][0]+arr[0][1]+arr[0][2]+arr[1][1]+arr[2][0]+arr[2][1]+arr[2][2];
    cout<<sum;
}
